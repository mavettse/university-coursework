vsim work.mux2por1
# vsim work.mux2por1 
# Loading work.mux2por1
add wave -position insertpoint  \
sim:/mux2por1/I0
wave create -driver freeze -pattern clock -initialvalue HiZ -period 100ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/mux2por1/I0
wave create -driver freeze -pattern clock -initialvalue HiZ -period 200ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/mux2por1/I1
wave create -driver freeze -pattern clock -initialvalue HiZ -period 400ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/mux2por1/sel
add wave -position insertpoint  \
sim:/mux2por1/s
run -all
step -over
