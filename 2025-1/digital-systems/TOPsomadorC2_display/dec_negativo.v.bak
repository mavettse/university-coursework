module sinalMenos(bitSinal, seg);
	input bitSinal;
	output [6:0] seg;
			
	wire sega, segb, segc, segd, sege, segf, segg;

	assign sega = 1;
	assign segb = 1;
	assign segc = 1;
	assign segd = 1;
	assign sege = 1;
	assign segf = 1;
	assign segg = ~bitSinal;

	assign seg = {segg, segf, sege, segd, segc, segb, sega};		
	
endmodule